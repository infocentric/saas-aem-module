{
  "name": "@valtech-ch/saas",
  "version": "0.0.10",
  "description": "Frontend of the Valtech CH Search as a Service module",
  "repository": {
    "type": "git",
    "url": "https://github.com/valtech-ch/saas-aem-module"
  },
  "author": {
    "name": "Valtech Switzerland AG",
    "url": "https://www.valtech.com/"
  },
  "main": "package/saas.js",
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "dev": "webpack --env dev --config ./webpack.dev.js && clientlib --verbose",
    "prod": "webpack --config ./webpack.prod.js && clientlib --verbose",
    "start": "webpack-dev-server --open --config ./webpack.dev.js",
    "sync": "aemsync -d -p ../ui.apps/src/main/content",
    "watch": "webpack-dev-server --config ./webpack.dev.js & watch 'clientlib' ./dist & aemsync -w ../ui.apps/src/main/content",
    "lint": "eslint . --ext .js,.ts",
    "lint-fix": "eslint . --ext .js,.ts --fix",
    "test": "jest",
    "prepare": "cd .. && husky install",
    "lint-staged": "lint-staged",
    "package": "webpack --config ./webpack.npm.js",
    "release": "npm publish --access public",
    "version": "git add package.json package-lock.json && git commit -m 'Bump version'",
    "postversion": "git push"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "@valtech-ch/eslint-config": "^2.2.0",
    "aem-clientlib-generator": "1.7.7",
    "aemsync": "4.0.3",
    "autoprefixer": "10.4.0",
    "babel-loader": "^8.2.3",
    "browserslist": "4.18.1",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "9.1.0",
    "css-loader": "^3.0.0",
    "cssnano": "5.0.10",
    "eslint": "8.2.0",
    "eslint-config-airbnb-typescript": "15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "25.2.4",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-testing-library": "5.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "glob-import-loader": "^1.1.4",
    "html-webpack-plugin": "5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "12.0.2",
    "mini-css-extract-plugin": "2.4.4",
    "optimize-css-assets-webpack-plugin": "6.0.1",
    "postcss": "^8.3.11",
    "postcss-loader": "6.2.0",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "sass-loader": "12.3.0",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.2.5",
    "ts-loader": "9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.4.4",
    "watch": "^1.0.2",
    "webpack": "5.64.1",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.5.0",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {},
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": "npm run lint"
  }
}
